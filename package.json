    "better-sqlite3": "^12.4.1",
    "dotenv": "^17.2.3",
    "ethers": "^6.15.0",
    "node-cache": "^5.1.2",
    "openai": "^4.104.0",
    "uint8arrays": "^5.1.0",
    "viem": "^2.38.3"
  },
  "devDependencies": {
    "@eslint/compat": "^1.2.6",
    "@eslint/js": "^9.19.0",
    "@types/node": "^22.13.0",
